<% if section.custom %>
  <% if section.has_card %>
    <%= govuk_summary_card(title: section.title) do %>
      <%= render section.custom[:partial], **section.custom[:locals] %>
    <% end %>
  <% else %>
    <%= render section.custom[:partial], **section.custom[:locals] %>
  <% end %>
<% else %>
  <%= govuk_summary_list(
    card: { title: section.title },
    rows: section.rows
  ) %>
<% end %>
